{
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"esModuleInterop": true,
		"isolatedModules": true,
		"resolveJsonModule": true,
		"noEmit": false,
		"jsx": "preserve",
		"lib": ["dom", "dom.iterable", "esnext"],
		"outDir": "./out-tsc/app",
		"plugins": [
			{
				"name": "next"
			}
		]
	},
	"exclude": [
		"sst.config.ts",
		"jest.config.ts",
		"src/**/*.test.*",
		"src/**/*.spec.*"
	],
	"include": [
		"src",
		"*.ts",
		".next/types/**/*.ts",
		"next.config.js",
		"src/**/*.json"
	],
	"references": []
}
